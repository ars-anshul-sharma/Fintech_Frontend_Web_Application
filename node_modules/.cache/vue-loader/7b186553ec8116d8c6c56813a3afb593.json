src/components/Step2","sourcesContent":["<template>\n<div>\n  \n    <div class=\"row overlay\">\n      <div class=\"col-2.10\">\n            <div class=\"sidebar\">\n        <div class=\"logo-details\">\n          <i class='bx bx-credit-card-alt' ></i>\n          <span class=\"logo_name\">Rakuten Card</span>\n        </div>\n            <ul class=\"nav-links\">\n              <li id=\"op1\" class=\"selectedBg\">\n                <div class=\"iocn-link\">\n                  <label for=\"a1\" id=\"label1\">\n                    <i class='bx bx-collection'></i>\n                      <input class=\"rmRadio\" type=\"radio\" id=\"a1\" name=\"q1\" @click=\"changeclr1\"/>UserDetails\n                  </label>\n                </div>\n              </li>\n              <li id=\"op2\">\n                <div class=\"iocn-link\">\n                  <label for=\"a2\" id=\"label1\">\n                    <i class='bx bx-collection'></i>\n                      <input class=\"rmRadio\" type=\"radio\" id=\"a2\" name=\"q1\" @click=\"changeclr2\"/>Family\n                  </label>\n                </div>\n              </li>\n              <li id=\"op3\">\n                <div class=\"iocn-link\">\n                  <label for=\"a3\" id=\"label1\">\n                    <i class='bx bx-collection'></i>\n                      <input class=\"rmRadio\" type=\"radio\" id=\"a3\" name=\"q1\" @click=\"changeclr3\"/>Work Information\n                  </label>\n                </div>\n              </li>\n              <li id=\"op4\">\n                <div class=\"iocn-link\">\n                  <label for=\"a4\" id=\"label1\">\n                    <i class='bx bx-collection' ></i>\n                      <input class=\"rmRadio\" type=\"radio\" id=\"a4\" name=\"q1\" @click=\"changeclr4\"/> Confirmation \n                  </label>\n                </div>\n              </li>\n          </ul>\n  </div>\n  </div>\n      <div class=\"col qwe\">\n\n            <div class=\"row\">\n              <div class=\"col\">\n\n                <span v-if=\"currentSelected == 3\" class=\"margin1\">\n                <OfficeName class=\"margin1\"/>\n                <Address class=\"margin1\"/>\n                </span>\n\n                <span v-if=\"currentSelected == 1\" class=\"margin1\">\n                  <Name class=\"margin1\"/><br>\n                  <GenderAndBirthday class=\"margin1\"/>\n                   <MailAndAddress class=\"margin1\"/>\n                </span>\n\n                <span v-if=\"currentSelected == 2\" class=\"margin1\">\n                  <AboutFamily1 class=\"margin1\"/>\n                </span>\n\n                <span v-if=\"currentSelected == 4\" class=\"margin1\">\n                  <ShowInfo class=\"margin1\"/>\n                </span>\n\n                </div>\n              <div class=\"col\">\n\n                <span v-if=\"currentSelected == 1\" class=\"margin1\">\n                <PhoneNumber class=\"margin1\"/>\n                <LengthOfService class=\"margin1\"/><br>\n                <Occupation class=\"margin1\"/>\n                <Profession class=\"margin1\"/><br><br>\n                <button type=\"button\" class=\"ml btn btn-m\" style=\"border-radius:5px; background-color:rgb(65, 140, 165); margin-bottom:50px;\" @click=\"changeclr2\">Next</button>\n                </span>\n\n                <span v-if=\"currentSelected == 3\" class=\"margin1\">\n                    <Step1Number class=\"margin1\"/><br>\n                    <Address class=\"margin1\"/>\n                    <button type=\"button\" class=\"ml btn btn-m\" style=\"border-radius:5px; background-color:rgb(65, 140, 165); margin-bottom:50px;\" @click=\"changeclr4\">Next</button>\n                </span>\n\n                <span v-if=\"currentSelected == 2\" class=\"margin1\">\n                  <AboutFamily2 class=\"margin1\"/>\n                  <button type=\"button\" class=\"ml btn btn-m\" style=\"border-radius:5px; background-color:rgb(65, 140, 165); margin-bottom:
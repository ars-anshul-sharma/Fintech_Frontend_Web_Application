>\n      </b-row>\n      <b-button\n        class=\"compareBtn\"\n        v-if=\"showCompareBtn\"\n        v-b-modal.cardComparisonModal\n        >Compare Selected</b-button\n      >\n      <!-- <b-button class=\"details\" v-b-modal=\"cardComparisonModal\">View Details</b-button> -->\n\n      <b-modal size=\"xl\" id=\"cardComparisonModal\" title=\"Card Comparison\">\n        <table id=\"comparisonTable\">\n          <thead>\n            <tr>\n              <th class=\"w-20\"></th>\n              <th class=\"w-35\" v-for=\"card in compareCards\" :key=\"card.id\">\n                {{ card.name }}\n              </th>\n              <!-- <th class=\"w-35\">Card 2</th> -->\n            </tr>\n          </thead>\n          <tbody>\n            <tr>\n              <th>Card</th>\n              <td v-for=\"card in compareCards\" :key=\"card.id\">\n                <div class=\"card-img-holder\">\n                  <img\n                    v-bind:src=\"require('../assets/cards/' + card.image)\"\n                    alt=\"\"\n                    class=\"w-35 wd-50\"\n                  />\n                </div>\n              </td>\n            </tr>\n            <tr\n              v-for=\"(comparable, index) in flattenedComparedCards\"\n              :key=\"index\"\n            >\n              <th>{{ comparable.name }}</th>\n              <td v-for=\"value in comparable.values\" :key=\"value\">\n                <ul>\n                  <li v-for=\"item in value\" :key=\"item\">{{ item }}</li>\n                </ul>\n              </td>\n            </tr>\n          </tbody>\n        </table>\n\n        <template #modal-footer=\"{ cancel }\">\n          <b-button\n            size=\"sm\"\n            variant=\"danger\"\n            class=\"float-left\"\n            @click=\"cancel()\"\n            >Close</b-button\n          >\n          <!-- <b-button size=\"sm\" variant=\"danger\" class=\"float-right ml-auto\" href=\"https://www.google.com\" target=\"_blank\">Apply!</b-button> -->\n        </template>\n      </b-modal>\n    </b-container>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"CompareBar\",\n  props: [\"showCompare\", \"compareCards\"],\n  methods: {\n    flattenComparedCards(newValue) {\n      let flatMap = [];\n      let x = newValue[0];\n      let y = newValue[1];\n      for (let i = 0; i < newValue[0].comparables.length; i++) {\n        let tmp = {};\n        tmp.name = x.comparables[i].name;\n        tmp.values = [x.comparables[i].values, y.comparables[i].values];\n        flatMap.push(tmp);\n      }\n      this.flattenedComparedCards = flatMap;\n    },\n  },\n  watch: {\n    compareCards: {\n      deep: true,\n      handler: function (newValue) {\n        if (newValue.length === 2) {\n          this.showCompareBtn = true;\n          this.flattenComparedCards(newValue);\n        } else {\n          this.showCompareBtn = false;\n        }\n      },\n    },\n    showCompare: {\n      deep: true,\n      handler: function (newValue) {\n        if (!newValue) {\n          for (let i = 0; i < this.compareCards.length; i++) {\n            this.compareCards[i].selected = false;\n          }\n        }\n      },\n    },\n  },\n};\n</script>\n\n<style scoped>\n.compareList {\n  background-color: white;\n}\n</style>"]}]}
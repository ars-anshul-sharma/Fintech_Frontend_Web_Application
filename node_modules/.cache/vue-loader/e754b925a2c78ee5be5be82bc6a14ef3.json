title=\"card.name\"\n      v-bind:class=\"card.flipped ? 'flip-container flipped' : 'flip-container'\"\n    >\n    <b-form-checkbox size=\"xl\" class=\"inpCompare\" v-if=\"showCompare\" :id=\"card.id\" :value=\"card.id\" v-model=\"comparisonSelection\" @click=\"card.selected = !card.selected\">\n    </b-form-checkbox>\n    \n      <div class=\"flipper\">\n        <div class=\"front\">\n          <slot name=\"front\"></slot>\n          <v-icon\n            class=\"frontFlipBtn\"\n            v-on:click=\"card.flipped = true\"\n            v-bind:title=\"card.name\"\n          >\n            <img class=\"info\" src=\"../assets/information-button.png\" />\n            <img\n              class=\"card-image\"\n              v-bind:src=\"require('../assets/cards/' + card.image)\"\n            />\n          </v-icon>\n        </div>\n\n        <div class=\"back\">\n          <slot name=\"back\"></slot>\n          <p class=\"card-desc\">\n            Easy-to-hold gold card with an annual membership fee of only 2,200\n            yen (tax included)\n            <br />\n            <ul>\n              <li>Domestic airport lounge is free twice a year</li>\n              <li>No annual membership fee for ETC card</li>\n            </ul>\n          </p>\n          <div id=\"actions\">\n            <b-button\n              class=\"backFlipBtn close\"\n              v-on:click=\"card.flipped = false\"\n              >Close</b-button\n            >\n            <b-button class=\"details\" v-b-modal=\"'modal' + card.id\"\n              >View Details</b-button\n            >\n\n            <b-modal\n              size=\"xl\"\n              :id=\"'modal' + card.id\"\n              v-bind:title=\"card.name\"\n              body-class=\"modal-bg\"\n            >\n              <h2>Features</h2>\n              <ul class=\"double-column\">\n                <li v-for=\"feature in card.features\" :key=\"feature\">\n                  {{ feature }}\n                </li>\n              </ul>\n              <h2>Benefits</h2>\n              <b-row>\n                <b-col\n                  v-for=\"comparable in card.comparables\"\n                  :key=\"comparable.name\"\n                  md=\"4\"\n                  class=\"benefitCol\"\n                >\n                  <h5 class=\"benefitHeading\">{{ comparable.name }}</h5>\n                  <ul class=\"noBullet\">\n                    <li v-for=\"benefit in comparable.values\" :key=\"benefit\">\n                      {{ benefit }}\n                    </li>\n                  </ul>\n                </b-col>\n              </b-row>\n              <template #modal-footer=\"{ cancel }\">\n                <b-button\n                  size=\"sm\"\n                  variant=\"primary\"\n                  class=\"float-left\"\n                  @click=\"cancel()\"\n                  >Cancel</b-button\n                >\n                <b-button\n                  size=\"sm\"\n                  variant=\"danger\"\n                  class=\"float-right ml-auto\"\n                  href=\"https://www.google.com\"\n                  target=\"_blank\"\n                  >Apply!</b-button\n                >\n              </template>\n            </b-modal>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"Card\",\n  props: [\"card\", \"showCompare\", \"comparisonSelection\"],\n  // {\n  //   card: Object,\n  //   showCompare: Boolean,\n  //   comparisonSelection: Array,\n  // },\n  watch: {\n    comparisonSelection: {\n      deep: true,\n      handler: function (newValue) {\n        this.$emit(\"update:comparisonSelection\", newValue);\n        console.log(\"Comparison Selection Card\", newValue);\n      },\n    },\n  },\n  data: function () {\n    return {};\n  },\n};\n</script>\n\n<style scoped>\n.inpCompare {\n  position: absolute;\n  z-index: 100;\n  zoom: 2;\n  float: right;\n}\n</style>"]}]}